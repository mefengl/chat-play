{
  "name": "chatkit",
  "author": "mefengl",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist",
    "chatgpt.js",
    "chatgpt.d.ts",
    "bing.js",
    "bing.d.ts",
    "bard.js",
    "bard.d.ts"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/chatgpt/index.ts src/bing/index.ts src/bard/index.ts --format esm,cjs --dts",
    "dev": "npm run build -- --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-custom": "workspace:*",
    "tsup": "latest",
    "typescript": "latest",
    "tsconfig": "workspace:*",
    "@types/tampermonkey": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
